{
  "name": "node-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/bin/www",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel --presets es2015 -d ./build ./server -s && cp -a ./server/public ./build && gulp rename && gulp clean && cp -a ./server/bin ./build"
  },
  "dependencies": {
    "babel": "^5.8.9",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-react-views": "^0.10.2",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.2",
    "gulp-rename": "^1.2.2",
    "morgan": "~1.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "serve-favicon": "~2.3.0"
  }
}
